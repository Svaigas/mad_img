							"ops": "*",
							"response": "*",
							"wreck": "*"
						}]
					}, {
						"module": "good-console"
					}, "stdout"]
				}
			}
		}
	}, {
		"plugin": {
			"register": ".",
			"options": {
				"name": "mad_img"
			}
		},
		"options": {
			"routes": {
				"prefix": "/image/v1"
			}
		}
	}]
}