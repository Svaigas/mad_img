{
	"connections": [{
		"port": 8010,
		"routes": {
			"log": true
		}
	}],
	"registrations": [{
		"plugin": {
			"register": "good",
			"options": {
				"ops": {
					"interval": 60000
				},
				"reporters": {
					"consoleReporter": [{
						"module": "good-squeeze",
